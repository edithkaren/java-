<%@page contentType="text/html" pageEncoding="UTF-8"%>
<!DOCTYPE html>
<html lang="en">
    <head>
        <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
        <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no"> 
        <meta name="author" content="ColorLib"> 
        <meta name="KeyWords" content="ColorLib Templates"> 
     
    <!-- Title Page-->
    <title>IBM-Link7: SVIIT-SVICA Internship 20-21 Lock-1st Internal Evaluation report </title>
    <!-- main.css--> 
    <link href="css/main.css" rel="stylesheet" media="all"> 
</head> 
<body> 
    <div class="page-wrapper bg-gra-03 p-t-45 p-b-50">
        <div class="wrapper wrapper--w790">
            <div class="card card-5">
                <div class="card-heading"> 
                    <div class="card-heading">
                                <h2 class="title">IBM-Link7: SVIIT-SVICA Internship 20-21 Lock-1st Internal Evaluation report
                                   </h2>
                               
                            </div>
                </div>

                <div class="card-body">

                    <div class="form-row">

                        <div class="">
                                 <li> 
                                    <p> The name and photo associated with your Google account will be recorded when you upload files and submit this form
                                    </p> 
                                </li>

                    </div>
                   
                </div>
            </div>
        </div><br>
    
    <div class="page-wrappet bg-gra-03 p-t-45 p-b-50"> 
        <div class="wrapper wrapper--w790"> 
            <div class="card card-5">    
                <div class="card-heading"> 
                     <h2 class="title">Details of 1st Internal Evaluation report</h2>
                </div>
                <div class="card-body">
                    
                        <div class="form-row m-b-55">
                            <div class="label">Internal Mentor Mobile NO(Without +91 and Space)</div>
                            <div class="value">
                                <div class="row row-space">
                                    <div class="col-2">
                                        <div class="input-group-desc">
                                            <input class="input--style-5" type="text" name="Number" size="60">
                                            <label class="label--desc">Number</label>
                                        </div>
                                    </div>
                   
                        
                                    
                                    <div class="form-row p-t-20">
                            <label class="label">Uploading Evaluation Report Based on 1FNR and 2FNR</label>
                            <div class="p-t-15">
                                <label class="radio-container m-r-55">1st Internal Report
                                    <input type="radio"  name="exist">
                                    <span class="checkmark"></span>
                                </label>
                            </div> 
                       </div>
                        
                       
                        
                        <div class="form-row p-t-20">
                            <div class="label"> Upload Plagiarism Report(1file/1MB) </div>
                            <div class="p-t-15">
                                <label class="Upload">Add File 
                                    <input type="File" name="File"  size="20" >
                                </label>
                            </div>
                        </div>  
                               
                            <div class="form-row m-b-55">
                            <div class="label">Originality in percentages(Refer Plagiarism Report)</div>
                            <div class="value">
                                <div class="row row-space">
                                    <div class="col-2">
                                        <div class="input-group-desc">
                                            <input class="input--style-5" type="text" name="Number">
                                            <label class="label--desc"></label>
                                        </div>
                                    </div>
   
                                    
                              <div class="form-row p-t-20">
                            <label class="label label--block">Grade</label>
                            <div class="p-t-15">
                                <label class="radio-container m-r-55">A(Range 100 to 81)
                                    <input type="radio"  name="exist">
                                    <span class="checkmark"></span>
                                </label> 
                            </div>  
                            <br>
                            <div class="p-t-15">
                                <label class="radio-container m-r-55">B(Range 80 to 61)
                                    <input type="radio"  name="exist">
                                    <span class="checkmark"></span>
                                </label>  
                            </div> 
                            <br>
                            <div class="p-t-15">
                                <label class="radio-container m-r-55">C(Range 60 to 41)
                                    <input type="radio"  name="exist">
                                    <span class="checkmark"></span>
                                </label> 
                            </div>  
                            <br>
                            <div class="p-t-15">
                                <label class="radio-container m-r-55">D(Range 40 to 21)
                                    <input type="radio"  name="exist">
                                    <span class="checkmark"></span>
                                </label> 
                            </div> 
                            <br>
                            <div class="p-t-15">
                                <label class="radio-container m-r-55">E(Range 20 to 0)
                                    <input type="radio"  name="exist">
                                    <span class="checkmark"></span>
                                </label> 
                            </div>  
                            <br>
                            <div class="p-t-15"> <br>
                                <label class="radio-container m-r-55">Rejected
                                    <input type="radio"  name="exist">
                                    <span class="checkmark"></span>
                                </label> 
                            </div>   
                            <br> 
                            <div class="p-t-15">
                                <label class="radio-container m-r-55">Repeat
                                    <input type="radio"  name="exist">
                                    <span class="checkmark"></span>
                                </label> 
                            </div>
                       </div>  
                                    
                        <div class="form-row">
                            <div class="name">Score (out of 100)</div>
                            <div class="value">
                                <div class="input-group">
                                    <input class="input--style-5"  type="number"  name="number" min="1" max="100"> 
                                </div>
                            </div>
                        </div>             
                                    
                                    <form method="POST" action="result.html" onclick="this.value" > 
                                        <div>
                            <button class="btn btn--radius-2 btn--red" type="button"  >Back</button>
                                        </div> <br>            
                        <div> 
                            <button class="btn btn--radius-2 btn--red" type="submit"> Submit</button>
                        </div> 
                    </form>
                </div>
            </div>  
</body>
    
</html>

